<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
    </style>
    
    
    
     
      <style name="Bar" parent="@android:style/TextAppearance.Medium">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">#0000A0</item>
    <item name="android:textSize">10pt</item>
    <item name="android:background">#B6B6B4</item>
    
  </style>
    
     <style name="BarInfo" parent="@android:style/TextAppearance.Medium">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">#254117</item>
    <item name="android:textSize">10pt</item>
    <item name="android:background">#EBDDE2</item>
  </style>
     
    
    
    
    
    
    
    
    
    

